msgid "Win payout if %1 touches %4 through %3 after %2."
msgstr "পে-আউট জিতুন যদি %1 %2 এর পরে %4 থেকে %3 পর্যন্ত স্পর্শ করে।"

msgid "Win payout if %1 touches %4 through %3."
msgstr "পেআউট জিতুন যদি %1 %4 থেকে %3 স্পর্শ করে।"

msgid "Win payout if %1 touches %4 through %plural(%3,%d tick,%d ticks) after %2."
msgstr "%2-এর পর %1 %4-কে %বহুবচন(%3,%d টিক, %d টিক) স্পর্শ করলে পেআউট জিতুন।"

msgid "Win payout if the last digit of %1 is %4 after %plural(%3,%d tick,%d ticks)."
msgstr "%1 এর শেষ ডিজিট যদি %4 এর পরে %3 হয় (%3,%d টিক,%d টিক)।"

msgid "Win payout if the last digit of %1 is even after %3 ticks."
msgstr "যদি %1 এর শেষ সংখ্যাটি %3 টিক করার পরেও হয় তাহলে পেআউট জিতুন।"

msgid "Win payout if the last digit of %1 is not %4 after %plural(%3,%d tick,%d ticks)."
msgstr "%1-এর শেষ সংখ্যা %4 না হলে %বহুবচনের (%3,%d টিক,%d টিক) পরে পেআউট জিতুন।"

msgid "Win payout if the last digit of %1 is odd after %3 ticks."
msgstr "%3 টিক করার পর যদি %1 এর শেষ সংখ্যাটি বিজোড় হয় তাহলে পেআউট জিতুন।"

msgid "Win payout if the last digit of %1 is strictly higher than %4 after %3 ticks."
msgstr "%3 টিক করার পর যদি %1-এর শেষ ডিজিট %4-এর থেকে বেশি হয় তাহলে পেআউট জিতুন।"

msgid "Win payout if the last digit of %1 is strictly lower than %4 after %3 ticks."
msgstr "%3 টিক করার পর %1-এর শেষ সংখ্যাটি %4-এর থেকে কঠোরভাবে কম হলে পেআউট জিতুন।"

msgid "Win payout if the last tick of %1 is strictly higher than the average of the %plural(%3,%d tick,%d ticks)."
msgstr "%1-এর শেষ টিক %বহুবচনের (%3,%d টিক,%d টিক) গড় থেকে কঠোরভাবে বেশি হলে পেআউট জিতুন।"

msgid "Win payout if the last tick of %1 is strictly lower than the average of the %plural(%3,%d tick,%d ticks)."
msgstr "যদি %1 এর শেষ টিকটি %বহুবচনের (%3,%d টিক,%d টিক) গড় থেকে কঠোরভাবে কম হয় তাহলে পেআউট জিতুন।"

msgid "Win payout if tick %5 of %1 is the highest among all %3 ticks."
msgstr "সমস্ত %3 টিকগুলির মধ্যে %1 এর %5 টিক দিয়ে পেআউট জিতুন।"

msgid "Win payout if tick %5 of %1 is the lowest among all %3 ticks."
msgstr "যদি %1 এর %5 টিক সব %3 টিকের মধ্যে সর্বনিম্ন হয় তাহলে পেআউট জিতুন।"

msgid "Win up to %7 %6 if %1's exit tick is between %4 and %5 after %plural(%3,%d tick,%d ticks)."
msgstr "%7 %6 পর্যন্ত জিতুন যদি %1 এর এক্সিট টিক %4 এবং %5 এর মধ্যে হয় %বহুবচন(%3, %d টিক, %d টিক) এর পরে।"

msgid "Win up to %7 %6 if %1's exit tick is between %4 and %5 at %3 after %2."
msgstr "%7 %6 পর্যন্ত জিতুন যদি %2-এর পরে %3-এ %1-এর প্রস্থান টিক %4 এবং %5-এর মধ্যে হয়।"

msgid "Win up to %7 %6 if %1's exit tick is between %4 and %5 at %3."
msgstr "%7 %6 পর্যন্ত জিতুন যদি %1-এর প্রস্থান টিক %3-এ %4 এবং %5-এর মধ্যে হয়।"

msgid "Win up to %7 %6 if %1's exit tick is between %5 and %4 after %plural(%3,%d tick,%d ticks)."
msgstr "%7 %6 পর্যন্ত জিতুন যদি %1 এর এক্সিট টিক %5 এবং %4 এর মধ্যে হয় %plural(%3,%d টিক, %d টিক) এর পরে।"

msgid "Win up to %7 %6 if %1's exit tick is between %5 and %4 at %3 after %2."
msgstr "%7 %6 পর্যন্ত জিতুন যদি %2-এর পরে %3-এ %1-এর প্রস্থান টিক %5 এবং %4-এর মধ্যে হয়।"

msgid "Win up to %7 %6 if %1's exit tick is between %5 and %4 at %3."
msgstr "%7 %6 পর্যন্ত জিতুন যদি %1-এর প্রস্থান টিক %3-এ %5 এবং %4-এর মধ্যে হয়।"

