msgid "If this was you, you can ignore this email."
msgstr "Se l'operazione è stata effettuata da te, ti invitiamo a ignorare la presente e-mail."

msgctxt "Customer.io email: svg_all_transactional_unknown_login "
msgid "%1 If you do not recognize this activity, go to %2%3%4 to reset your password immediately. %5"
msgstr "%1 Se le attività non sono riconducibili a te, reimposta subito la tua password su %2%3%4. %5"

msgctxt "Customer.io email: svg_all_transactional_unknown_login "
msgid "Security alert: New sign-in activity"
msgstr "Avviso di sicurezza: Nuova attività di accesso"

msgctxt "Customer.io email: svg_all_transactional_binary_unknown_login "
msgid "Binary.com"
msgstr "Binary.com"

msgctxt "Customer.io email: svg_all_transactional_binary_unknown_login "
msgid "Hello %1,"
msgstr "Ciao %1,"

msgctxt "Customer.io email: svg_all_transactional_binary_unknown_login "
msgid "We detected a new sign-in on your account."
msgstr "È stata rilevata una nuova registrazione sul tuo conto."

msgctxt "Customer.io email: svg_all_transactional_binary_unknown_login "
