msgid "Device:"
msgstr "Төхөөрөмж:"

msgctxt "Customer.io email: svg_all_transactional_unknown_login "
msgid "Location:"
msgstr "Байршил:"

msgctxt "Customer.io email: svg_all_transactional_unknown_login "
msgid "IP address:"
msgstr "IP хаяг:"

