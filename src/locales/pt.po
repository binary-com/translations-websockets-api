msgstr "Extrato bancário"

msgctxt "Customer.io email: svg_all_transactional_poa_failed_reminder "
msgid "Your document must show your full name."
msgstr "O documento deve incluir o seu nome completo."

msgctxt "Customer.io email: svg_all_transactional_poa_failed_reminder "
msgid "If you have any questions, %1contact us via live chat%2."
msgstr "Em caso de dúvidas, %1contacte-nos via live chat.%2."

msgctxt "Customer.io email: svg_all_transactional_poa_failed_reminder "
msgid "Reminder to submit a valid proof of address"
msgstr "Não se esqueça de submeter um comprovativo válido de morada."

