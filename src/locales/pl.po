msgstr "Wprowadź kwotę wpłaty i kliknij %1Dalej%2."

msgctxt "Customer.io email: svg_botswana_attempt_failed_BIN_471396_Skrill "
msgid "Step 4:"
msgstr "Krok 4:"

msgctxt "Customer.io email: svg_botswana_attempt_failed_BIN_471396_Skrill "
